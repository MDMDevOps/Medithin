if(self.options.slideup_slidefrom=="T"||self.options.slideup_slidefrom=="B"){htmlbox.style.top=pos+"px";}else{htmlbox.style.left=pos+"px";}
if(stop==1){return;}
setTimeout(function(){self.htmlboxMove(delta,finishpos);},10);}}
