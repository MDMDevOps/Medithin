this.closetimeout=function(timeout){if(timeout<=1){var dv=document.getElementById('htmlbox');dv.style.display='none';}else{timeout=parseInt(timeout)-1;tsp=document.getElementById("htmlbox_timeout");if(tsp){tsp.innerHTML=timeout;}
