this.registerWidget=function(widgetName,obj)
{self.regesteredWidgets[widgetName]=obj}
this.ajaxLoadWidgetUrl=function(widgetName,actionName)
{if(typeof self.regesteredWidgets[widgetName]!='undefined'){self.ajaxLoadUrl(self.regesteredWidgets[widgetName].url+'.'+actionName+'?'+$.param(self.regesteredWidgets[widgetName].params));}}
this.ajaxLoadUrlHistory=function(url,scrollto,e)
{var execute=function(){if(self.appOptions.resourcesVersion!=self.appOptions.resourcesVersionCurrent){window.location=url;}else{url=decodeURIComponent(url);if(url!=self.appOptions.currentUrl){if(window.History.enabled){History.pushState({scrollto:scrollto},document.title,url);}else{self.ajaxLoadUrl(url,scrollto,e);self.appOptions.currentUrl=url;}}else{self.ajaxLoadUrl(url,scrollto,e);}}}
if(self.isFormDirty()){jConfirm('Your data has not been saved. Are you sure you want to leave this page?','Please confirm this action',function(cResult){if(cResult){execute();}});}else{execute();}}
this.ajaxLoadUrlModal=function(url,scrollto,e)
{self.saveBgClass();self.ajaxLoadUrl(url,scrollto,e);}
this.ajaxLoadUrl=function(url,scrollto,e)
{if(typeof e!='undefined'&&$(e.target).is('.autoclose-modal')){self.closeModal();}else if(typeof e!='undefined'&&$(e.target).is('.autoclose-popup')){$.alerts._hide();}
$.ajax({"dataType":"json","headers":{Accept:'application/json'},"accept":'application/json',"url":url,"type":"GET","success":function(data){self.ajaxSuccess('',data,url,scrollto);},"error":self.ajaxError,"cache":false});}
this.isWidgetUrl=function(url)
{if(url){var spl=url.split('?'),params=$.deparam.querystring('?'+(spl[1]||''));if(params.widgetmarker)
return true;else
return false;}else{return false;}}
this.btnAjaxBeforeSend=function(element)
{if(typeof element.tagName!='undefined'&&element.tagName.toLowerCase()=='button'){var btn=$(element);btn.attr('disabled',true);self.btnWidth=btn.width();self.btnLabel=btn.html();btn.html('<i class="icon-upload"></i>');btn.width(self.btnWidth);}
if(typeof element.tagName!='undefined'){$(element).addClass('in_progress_post');}
$('.lock-full').addClass('in_progress_request');$('.ajax-loader-progress').show();}
this.btnAjaxSuccess=function(element)
{if(element.tagName.toLowerCase()=='button'){var btn=$(element);btn.attr('disabled',false);self.btnWidth=btn.width();btn.html(self.btnLabel);btn.width(self.btnWidth);}
$(element).removeClass('in_progress_post');$('.lock-full').removeClass('in_progress_request');}
this.ajaxError=function(jqXHR,textStatus,errorThrown)
{if(jqXHR.responseText){alert(jqXHR.responseText);}
if(self.isButtonContext(this)){self.btnAjaxSuccess(this);}}
this.getUrl=function(element)
{var url;if($(element).attr('data-href')){url=$(element).attr('data-href');}else if($(element).attr('href')){url=$(element).attr('href');}else if(($(element).attr('type'))&&($(element).attr('type')=='submit')&&($(element).parents('form'))&&($(element).parents('form').attr('action'))){url=$(element).parents('form').attr('action');}else{url=self.appOptions.currentUrl;}
var widget;if(widget=$(element).data('widget')){if(typeof self.regesteredWidgets[widget].params!='undefined'){url+='?'+$.param(self.regesteredWidgets[widget].params);}}
return url;}
this.ajaxPost=function(element)
{if($(element).attr('data-confirm')){if(!$(element).data('confirmed')){jConfirm($(element).attr('data-confirm'),'Please confirm this action',function(cResult){if(cResult){$(element).data('confirmed',true);self.ajaxPost(element);}});}
if($(element).data('confirmed')){$(element).data('confirmed',false)}else{return false;}}
var parent_form=$(element).parents("form");if(parent_form.length==0||$(element).is('.no_get_form')){var data=$(element).data(),method=false;data[self.appOptions.csrfTokenName]=self.appOptions.csrfTokenValue;if(data['confirm']!='undefined')
delete data['confirm'];if(data['confirmed']!='undefined')
delete data['confirmed'];}else{var data,method=parent_form.attr('method');if(parent_form.find('input[name='+self.appOptions.csrfTokenName+']').length==0){parent_form.prepend($('<input>',{'type':'hidden','name':self.appOptions.csrfTokenName,'value':self.appOptions.csrfTokenValue}));}
if(method=='get'){parent_form.find('input[name='+self.appOptions.csrfTokenName+']').remove();}
if(typeof tinyMCE!='undefined'){tinyMCE.triggerSave();}
data=parent_form.serialize();}
var url=self.getUrl(element);$.ajax({"dataType":"json","headers":{Accept:'application/json'},"accept":'application/json',"url":url,"type":method?method:"post","context":element,"beforeSend":self.ajaxBeforeSend,"success":function(data){self.ajaxSuccess(element,data,url);},"error":self.ajaxError,"data":data,"cache":false});return false;}
this.ajaxSuccess=function(element,data,url,scrollto)
{if(!data)var data={};if(typeof data.resourcesVersion!='undefined')
self.appOptions.resourcesVersionCurrent=data.resourcesVersion;if(self.getFormSelector(element)!==false){var formId=self.getFormSelector(element).attr('id');}
if(typeof data.metaTitle!='undefined'){$('title').html(data.metaTitle);}
if((typeof data.container!='undefined')&&(typeof data.content!='undefined')){if(typeof tinyMCE!='undefined'&&$(data.container).data('pagecontainer')){tinyMCE.remove();}
if($(data.container).data('pagecontainer')){$(window).off('scroll');$(window).off('resize');}
$(data.container).html(data.content);if(typeof scrollto!='undefined'){self.scrollPage(scrollto);}
self.initGlobalPlugins();}
if(typeof data.callback!='undefined'){$.globalEval(data.callback);}
if(typeof data.error!='undefined'){self.isFormHasError=true;var title;if(typeof data.errorTitle!='undefined'){title=data.errorTitle;}else{title='Error';}
jAlert(data.error,title);if(self.isButtonContext(element)){self.btnAjaxSuccess(element);}}
if((typeof data.soft_redirect!='undefined')){if(formId){$('#'+formId).removeClass('dirty');}
if(!data.soft_redirect)
data.soft_redirect=self.getUrl();if(typeof data.soft_redirect=='object'){if((typeof data.soft_redirect.skip_history!='undefined')&&(data.soft_redirect.skip_history))
self.ajaxLoadUrl(data.soft_redirect.url?data.soft_redirect.url:self.getUrl());else
self.ajaxLoadUrlHistory(data.soft_redirect.url?data.soft_redirect.url:self.getUrl(),data.soft_redirect.scrollto);}else{self.ajaxLoadUrlHistory(data.soft_redirect);}}else if((typeof data.redirect!='undefined')&&(data.redirect)){if(formId){$('#'+formId).removeClass('dirty');}
window.location=data.redirect;}
if(formId){if(self.isFormHasError){if(self.isFormDirtyBeforeRequest){$('#'+formId).addClass('dirty');}else{$('#'+formId).removeClass('dirty');}}else{$('#'+formId).removeClass('dirty');}
self.isFormDirtyBeforeRequest=null;self.isFormHasError=null;}
if(self.isWidgetUrl(url)==false){if((typeof data.bgclass!='undefined')&&(data.bgclass)){$('div#app-layouts-main-background-center').addClass(data.bgclass);}else{$('div#app-layouts-main-background-center').removeClass();}}
if(typeof data.hash1!='undefined'&&typeof data.hash2!='undefined'){$('#captcha_general').attr('src',data['url']);$('body').data('captcha.hash',[data['hash1'],data['hash2']]);}
$('.ajax-loader-progress').hide();}
this.ajaxBeforeSend=function()
{if(self.isButtonContext(this)){self.btnAjaxBeforeSend(this);self.isFormDirtyBeforeRequest=self.isFormDirty();self.isFormHasError=false;}}
this.isButtonContext=function(element)
{if(element==''){return false;}else if(typeof element.tagName=='undefined'){return false;}else{return true;}}
this.getFormSelector=function(element)
